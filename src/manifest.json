{
  "description": "description here",
  "icons": {},
  "manifest_version": 3,
  "host_permissions": ["http://*/*", "https://*/*"],
  "name": "Bilorino",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"]
    }
  ],
  "version": "1.0.1",
  "action": {
    "default_icon": "img/icon_active.png",
    "default_popup": "popup/popup.html",
    "default_title": "Bilorino"
  },
  "background": {
    "service_worker": "./background.js"
  },
  "content_security_policy": {}
}
