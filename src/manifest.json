{
  "name": "change-bilorino",
  "description": "A Vue.js web extension",
  "version": null,
  "permissions": [
    "tabs",
    "activeTab",
    "webNavigation",
    "*://*/*"
  ],
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "change-bilorino",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  }
}
